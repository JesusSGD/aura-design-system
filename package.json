    "@types/react": "^18.0.25",
    "next": "^13.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@ladle/react": "^2.4.5",
    "magic-sdk": "^10.1.0"
  }
