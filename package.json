{
  "name": "aura-design-system",
  "version": "1.1.2",
  "description": "Garitma's Design system",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "files": [
    "LICENSE",
    "README.md",
    "core"
  ],
  "author": "Garitma <hola@garitma.com",
  "license": "MIT",
  "homepage": "https://auradesignsystem.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garitma/aura-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/garitma/aura-design-system/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/react": "^6.1.17",
    "@testing-library/react": "^10.0.3",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^16.13.1"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "stylus": "stylus -w --compress stylus/style.styl -o stylus/style.css",
    "build-storybook": "build-storybook -c .storybook -o public",
    "build": "babel packages --out-dir core && npm run copy-types && stylus --compress stylus/style.styl -o core/style.css && stylus --compress stylus/vendors.styl -o core/vendors.css",
    "copy-types": "cp types/components/*.d.ts core",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"{packages,test,stories}/**/*.js\"",
    "git-reset": "git reset --hard HEAD",
    "git-clean": "git clean -d -x -e node_modules -e packages -f"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ui-library",
    "ui-components"
  ]
}
