  "scripts": {
    "stylus": "stylus -w --compress stylus/style.styl -o ./style.css",
    "build": "rm -rf dist && tsc --module commonjs && stylus --compress stylus/style.styl -o ./style.css && stylus --compress stylus/style.styl -o ./playground/public/sandbox.css && stylus --compress stylus/flex_columns.styl -o ./style_columns.css",
    "ladle": "npx ladle serve --stories=stories",
    "build-ladle": "npx ladle build --stories=stories"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "ui-library",
    "ui-components"
  ],
